{
	"name": "dw-file-check",
	"displayName": "DW File Check",
	"description": "DW-style file check-in/lock for deploy extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.checkInCurrentFile",
				"title": "Check In Current File"
			},
			{
				"command": "extension.checkOutCurrentFile",
				"title": "Check Out Current File"
			},
			{
				"command": "extension.dwPushCurrentFile",
				"title": "Push Current File"
			},
			{
				"command": "extension.dwPullCurrentFile",
				"title": "Pull Current File"
			},
			{
				"command": "extension.dwSaveCurrentFile",
				"title": "Save File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/username": "^3.0.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"event-stream": "^4.0.1",
		"fs": "0.0.1-security",
		"username": "^4.1.0"
	}
}
